{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "TBD"
  },
  "DicomStorage": {
    "BasePath": "/tmp/dicom-storage"
  },
  "AzureStorage": {
    "ConnectionString": "Default",
    "ContainerName": "dicomfiles",
    "BlobServiceEndpoint": "https://your_storage_account.blob.core.windows.net/",
    "AccountName": "your_storage_account",
    "AccountKey": "your_account_key_here",
    "UseManagedIdentity": false
  },
  "UseCloudStorage": false,
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "https://anothersite"
    ]
  }
}
